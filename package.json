{
  "name": "@elog/doc",
  "version": "1.0.0",
  "description": "Elog文档",
  "homepage": "https://github.com/LetTTGACO/elog/tree/master/packages/elog-doc#readme",
  "author": "1874 <i@1874.cool>",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LetTTGACO/elog.git"
  },
  "bugs": {
    "url": "https://github.com/LetTTGACO/elog/issues"
  },
  "scripts": {
    "copy": "cp ./docs/index.md ./docs/elog/index.md && cp ./docs/favicon.ico ./docs/elog/favicon.ico",
    "docs:dev": "pnpm copy && vitepress dev docs",
    "docs:build": "pnpm copy && vitepress build docs",
    "docs:serve": "pnpm copy && vitepress serve docs",
    "elog:init": "elog init",
    "elog:sync": "elog sync -e .env",
    "elog:clean": "elog clean"
  },
  "dependencies": {
    "@elog/cli": "0.3.0",
    "@types/node": "^18.11.9",
    "vitepress": "1.0.0-alpha.45",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
